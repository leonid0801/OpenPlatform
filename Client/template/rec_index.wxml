

<!-- 段子 -->
<template name="joke_tpl">
	<view class="list-item" bindtap="toast" id="{{item.f_id}}">
    <view class="item-body">
			<text>{{item.f_textarea}}</text>
		</view>
		<view class="item-header">
      
			<view class="header-img">
				<image class="img" src="{{item.f_avatar_url}}"></image>
			</view>

      <text class="header-name">{{item.f_nickname}}</text>
			<text class="header-time" >{{item.f_created}}</text>

		</view>
	</view>	
</template>
